<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="org.eventb.emf.core.presentation.CoreModelWizardID"
            name="%_UI_CoreModelWizard_label"
            class="org.eventb.emf.core.presentation.CoreModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/CoreModelFile.gif">
         <description>%_UI_CoreModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="org.eventb.emf.core.presentation.CoreEditorID"
            name="%_UI_CoreEditor_label"
            icon="icons/full/obj16/CoreModelFile.gif"
            extensions="prj"
            class="org.eventb.emf.core.presentation.CoreEditor"
            contributorClass="org.eventb.emf.core.presentation.CoreActionBarContributor">
      </editor>
   </extension>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="org.eventb.emf.core.machine.presentation.MachineModelWizardID"
            name="%_UI_MachineModelWizard_label"
            class="org.eventb.emf.core.machine.presentation.MachineModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/MachineModelFile.gif">
         <description>%_UI_MachineModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="org.eventb.emf.core.machine.presentation.MachineEditorID"
            name="%_UI_MachineEditor_label"
            icon="icons/full/obj16/MachineModelFile.gif"
            extensions="bum"
            class="org.eventb.emf.core.machine.presentation.MachineEditor"
            contributorClass="org.eventb.emf.core.machine.presentation.MachineActionBarContributor">
      </editor>
   </extension>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="org.eventb.emf.core.context.presentation.ContextModelWizardID"
            name="%_UI_ContextModelWizard_label"
            class="org.eventb.emf.core.context.presentation.ContextModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/ContextModelFile.gif">
         <description>%_UI_ContextModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="org.eventb.emf.core.context.presentation.ContextEditorID"
            name="%_UI_ContextEditor_label"
            icon="icons/full/obj16/ContextModelFile.gif"
            extensions="buc"
            class="org.eventb.emf.core.context.presentation.ContextEditor"
            contributorClass="org.eventb.emf.core.context.presentation.ContextActionBarContributor">
      </editor>
   </extension>

</plugin>

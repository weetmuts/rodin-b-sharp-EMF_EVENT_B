<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="ac.soton.eventb.emf.core.extension.navigator.itemProviderAdapterFactories" name="%extension-point.name" schema="schema/ac.soton.eventb.emf.core.extension.navigator.itemProvider.exsd"/>
   <extension
         point="org.eclipse.ui.navigator.navigatorContent">
      <navigatorContent
            contentProvider="ac.soton.eventb.emf.core.extension.navigator.provider.ExtensionContentProvider"
            id="ac.soton.eventb.emf.core.extension.navigator.content.emfContent"
            labelProvider="ac.soton.eventb.emf.core.extension.navigator.provider.ExtensionLabelProvider"
            name="%navigatorContent.name">
         <triggerPoints>
            <or>
               <instanceof
                     value="org.eventb.core.IEventBRoot">
               </instanceof>
               <instanceof
                     value="ac.soton.eventb.emf.core.extension.navigator.provider.ExtensionNavigatorItem">
               </instanceof>
            </or>
         </triggerPoints>
         <possibleChildren>
            <instanceof
                  value="ac.soton.eventb.emf.core.extension.navigator.provider.ExtensionNavigatorItem">
            </instanceof>
         </possibleChildren>
      </navigatorContent>
      <commonFilter
            activeByDefault="true"
            class="ac.soton.eventb.emf.core.extension.navigator.filter.AnnotationFilter"
            id="ac.soton.eventb.emf.core.extension.navigator.filter.annotationFilter"
            name="%commonFilter.name">
      </commonFilter>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.viewer">
      <viewerContentBinding
            viewerId="fr.systerel.explorer.navigator.view">
         <includes>
            <contentExtension
                  pattern="ac.soton.eventb.emf.core.extension.navigator.content.emfContent">
            </contentExtension>
            <contentExtension
                  pattern="ac.soton.eventb.emf.core.extension.navigator.filter.annotationFilter">
            </contentExtension></includes>
      </viewerContentBinding>
   </extension>

</plugin>
